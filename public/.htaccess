Header unset ETag
FileETag None

<FilesMatch "\.(ico|pdf|flv|jpg|jpeg|png|gif|bmp|swf|js|css)$">
	Header set Expires "Thu, 15 Apr 2000 20:00:00 GMT"
</FilesMatch>

<IfModule mod_deflate.c>
	<FilesMatch "\.(ico|pdf|flv|jpg|jpeg|png|gif|bmp|swf|js|css|php)$">
		SetOutputFilter DEFLATE
	</FilesMatch>
</IfModule>